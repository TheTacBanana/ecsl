use num::{int, float};
use bool::{bool};
use string::str;

fn print(s: str) {
    byt {
        PBP;
        LDR #8UB #s;
        PRINT_S;
    }
}

fn print_int(i: int) {
    byt {
        PBP;
        LDR #4UB #i;
        PRINT_I;
    }
}

fn print_float(f: float) {
    byt {
        PBP;
        LDR #4UB #f;
        PRINT_F;
    }
}

fn print_bool(b: bool) {
    byt {
        PBP;
        LDR #1UB #b;
        PRINT_B;
    }
}
